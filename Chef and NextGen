#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	std::cin >> t;
	while(t--){
	    int A,B;
	    cin>>A>>B;
	    int tobesupplied=A*B;
	    int X,Y;
	    cin>>X>>Y;
	    int totalsupply=X*Y;
	    if(totalsupply>=tobesupplied)
	    std::cout << "Yes" << std::endl;
	    else
	    std::cout << "no" << std::endl;
	    
	}

}
